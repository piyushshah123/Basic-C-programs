5.#include <stdio.h>

int main() {
    int number, factorial = 1, i = 1;


    printf("Enter an integer: ");
    scanf("%d", &number);

    
    while (i <= number) {
        factorial *= i;  // Multiply factorial by i
        i++;             // Increment i
    }

    
    printf("The factorial of %d is %d.\n", number, factorial);

    return 0;
}
